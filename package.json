{
  "name": "monobehaviour-name-matcher",
  "displayName": "MonoBehaviour Name Matcher",
  "description": "An extension for Unity devs to make editing MonoBehaviour file names easier.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "monobehaviour-name-matcher.fixSelectedMonoBehaviourName",
      "title": "Fix MonoBehaviour Name"
    }],
    "keybindings": [{
      "command": "monobehaviour-name-matcher.fixSelectedMonoBehaviourName",
      "key": "shift+alt+m",
      "mac": "shift+option+m",
      "when": "editorTextFocus"
    }]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.81.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}
